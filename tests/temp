#include <stdlib.h>
#include <stdio.h>
#include "main.h"


int main(int ac, char **av)
{
	int i;
	char id;

	if (ac != 2)
	{
		printf("Error\n");
		exit(EXIT_FAILURE);
	}

	i = 0;
	while(i < _strlen(av[1]))
	{
		if(av[1][i] == '%')
		{
			id = av[1][i+1];
			printf("%c\n", id);
		}
		i++;
	}
	exit(EXIT_SUCCESS);
}
